var array=[];
var count=1;

function fizzBuzz(){
    if (count%3===0 && count%5===0){
        array.push("FizzBuzz");
    } else if (count%3===0) {
        array.push("Fizz");
    } else if (count%5===0) {
        array.push("Buzz");
    }
    else {
        array.push(count);
    }
    count++;
}

var n=prompt("Enter limit:");
for (var i = 0; i < n; i++) {
  fizzBuzz();
}
console.log(array);

