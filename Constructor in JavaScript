function BellBoy (name,age,workPermit,languages) { //constructor in javascript
    this.name=name;
    this.age=age;
    this.workPermit=workPermit;
    this.languages=languages;
}

var bellBoy1 =new BellBoy ("timmy",21,"yes",["eng","span","turk"]);
console.log(bellBoy1.name);

function BellBoy(name,age,workPermit,language){
    this.name=name;
    this.age=age;
    this.workPermit=workPermit;
    this.language=language;

    this.moveSuitcase=function (){
        alert("moving in progress");
    }
}
